{
    "name": "bw/stand-with-ukraine-bundle",
    "description": "Shows StandWithUkraine banner and blocks content for Russian-speaking users",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BW\\StandWithUkraineBundle\\": "src/",
            "BW\\StandWithUkraineBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Victor Bocharsky",
            "email": "bocharsky.bw@gmail.com"
        }
    ],
    "require": {
        "symfony/dependency-injection": "^5.4|^6.0",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symfony/http-foundation": "^5.4|^6.0",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.3",
        "symfony/phpunit-bridge": "^5.4|^6.0"
    },
    "conflict": {
        "symfony/polyfill-php80": "<=1.23.0"
    },
    "config": {
        "sort-packages": true
    }
}
